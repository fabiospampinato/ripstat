    "name": "Fabio Spampinato",
    "email": "spampinabio@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fabiospampinato/ripstat.git"
  },
  "keywords": [
    "file",
    "stats",
    "fast"
  ],
  "dependencies": {
    "atomically": "^1.7.0"
  },
  "devDependencies": {
    "@types/node": "^14.14.37",
    "rimraf": "^3.0.2",
    "typescript": "^4.2.3"
  }
}
